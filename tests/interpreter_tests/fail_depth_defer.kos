const check = fun {
    var seq = 1

    return fun(value) {
        assert seq == value
        seq += 1
    }
} ()

fun() {
    defer {
        check(6)
    }
    defer {
        check(1)
        defer {
            check(2)
            defer {
                check(3)
                defer {
                    check(4)
                    defer {
                        check(5)
                    }
                    return
                }
                return
            }
            return
        }
        return
    }
    return
} ()
