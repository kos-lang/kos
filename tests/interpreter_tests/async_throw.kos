do {
    fun thrower
    {
        try {
            throw "hello"
        }
        catch const e {
        }
    }

    var i = 20
    while i {
        const t1 = thrower.async(void, [])
        const t2 = thrower.async(void, [])
        const t3 = thrower.async(void, [])
        const t4 = thrower.async(void, [])
        t1.wait()
        t2.wait()
        t3.wait()
        t4.wait()
        i -= 1
    }
}
