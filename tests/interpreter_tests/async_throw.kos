do {
    fun thrower
    {
        try {
            throw "hello"
        }
        catch const e {
        }
    }

    var i = 20
    while i {
        const t1 = async thrower()
        const t2 = async thrower()
        const t3 = async thrower()
        const t4 = async thrower()
        t1.wait()
        t2.wait()
        t3.wait()
        t4.wait()
        i -= 1
    }
}
