import base.buffer

do {
    var a = []
    var size = 1024 * 1024
    a.resize(1536 * 1024)
    var i = 0
    loop {
        try {
            a[i] = buffer(size)
            i += 1
        }
        catch const e {
            if size <= 512 {
                throw e
            }
            size >>= 1
        }
    }
}
