import base.print
import datetime.now

public const print_time = fun
{
    var last = void
    return fun(line)
    {
        const cur = now()
        if last != void {
            print("::\(line) - \(cur-last) us")
        }
        last = cur
    }
} ()

public fun expect_fail(function)
{
    var failed = false
    try {
        function()
    }
    catch const e {
        failed = true
    }
    assert failed
}

public var shared = []
