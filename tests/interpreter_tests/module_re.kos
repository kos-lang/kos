    const a = rg.find("0123aaabcbdbcefgh")
    assert a.begin == 6
    assert a.end   == 15
    assert a.groups.size     == 2
    assert a.groups[0]       != void
    assert a.groups[1]       != void
    assert a.groups[0].begin == 6
    assert a.groups[0].end   == 14
    assert a.groups[1].begin == 12
    assert a.groups[1].end   == 13
}

