import base.print
import base.each
import base.buffer

do {
    fun thrower
    {
        throw "raised"
    }

    try {
        thrower()
    }
    catch const e {
        e.print()
    }
}

print(void, true, false, "abc", -10)

print()

print("xyz", "", false, void, true, 0x1000, 1.2, ["a", 1], buffer([255, 0]))

"kos" -> each(print)

fun { yield 20; yield 30 ; yield "x" } () -> each(print)
