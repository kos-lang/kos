#!/usr/bin/env kos

import base: print, range

class new_fib {

    constructor {
        this.prev = 1
        this.cur  = 1
    }

    get_prev { return this.prev }
    get_cur  { return this.cur  }

    reset {
        this.prev = 1
        this.cur  = 1
    }

    set_next(next) {
        this.prev = this.cur
        this.cur  = next
    }

    advance {
        this.set_next( this.get_prev() + this.get_cur() )
    }

    execute(n) {
        for const i in range(n) {
            this.advance()
        }
        const ret = this.get_cur()
        this.reset()
        return ret
    }
}

const fib = new_fib()

const orig = fib.execute(50)

for const i in range(10000) {
    assert orig == fib.execute(50)
}

print(orig)
