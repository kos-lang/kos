/* @item debug backtrace()
 *
 *     backtrace()
 *
 * Returns backtrace from the place of the invocation.
 *
 * The backtrace returned is identical to the backtrace obtained
 * from an exception object.
 */
public fun backtrace
{
    try {
        throw void
    }
    catch const e {
        return e.backtrace[1:]
    }
}
